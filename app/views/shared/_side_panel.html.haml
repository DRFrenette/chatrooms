      %h4 Quick Contact
      .quick-contact-form-result
      %form#quick-contact-form.quick-contact-form.nobottommargin{:action => "include/quickcontact.php", :method => "post", :name => "quick-contact-form"}
        .form-process
        %input#quick-contact-form-name.required.sm-form-control.input-block-level{:name => "quick-contact-form-name", :placeholder => "Full Name", :type => "text", :value => ""}/
        %input#quick-contact-form-email.required.sm-form-control.email.input-block-level{:name => "quick-contact-form-email", :placeholder => "Email Address", :type => "text", :value => ""}/
        %textarea#quick-contact-form-message.required.sm-form-control.input-block-level.short-textarea{:cols => "30", :name => "quick-contact-form-message", :placeholder => "Message", :rows => "4"}
        %input#quick-contact-form-botcheck.hidden{:name => "quick-contact-form-botcheck", :type => "text", :value => ""}/
